{
    "name": "lqdi/rio-sgps",
    "description": "Sistema Gestor de Dados de Projetos Sociais - Prefeitura do Rio de Janeiro",
    "keywords": ["prefeitura", "social", "laravel"],
    "license": "AGPL",
    "type": "project",
    "require": {
        "php": "^7.2.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.0",
        "ixudra/curl": "^6.16",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.0",
        "spatie/laravel-blade-javascript": "^2.2",
        "spatie/laravel-collection-macros": "^3.8",
        "spatie/laravel-fractal": "^5.4",
        "spatie/laravel-tail": "^3.1",
        "tymon/jwt-auth": "^0.5.12",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "SGPS\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
